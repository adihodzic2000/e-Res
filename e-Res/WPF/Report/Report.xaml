<Window x:Class="WPF.Report.Report"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF.Report" xmlns:local1="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d"
      ResizeMode="NoResize" 
        Title="" Height="1000" Width="855" Closing="Window_Closing">
    <Grid>

        <WindowsFormsHost  >
            <local1:ReportViewer x:Name="ReportView" ></local1:ReportViewer>
        </WindowsFormsHost>
    </Grid>
</Window>
